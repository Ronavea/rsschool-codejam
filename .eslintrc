{
  "extends": "airbnb-base",
  "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
  },
  "parserOptions": {
      "ecmaVersion": 2017
  },
  "rules": {
      "linebreak-style": 0,
      "max-len": ["error", { "code": 200 }],
      "import/newline-after-import": "off"
  }
}